{
  "name": "alkemy-node",
  "version": "1.0.0",
  "description": "Desafio de nivelacion para alkemy, basado en Node.js con Express y Sequelize.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon server.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "alkemy",
    "challenge",
    "node",
    "express",
    "sequelize",
    "db"
  ],
  "author": "Martin Omar Heredia",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1"
  }
}
